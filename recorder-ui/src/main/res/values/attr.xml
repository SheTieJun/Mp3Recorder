<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SpreadView">
        <!--中心圆颜色-->
        <attr name="spread_center_color" format="color" />
        <!--中心圆半径-->
        <attr name="spread_radius" format="dimension" />
        <!--扩散圆颜色-->
        <attr name="spread_spread_color" format="color" />
        <!--扩散间距-->
        <attr name="spread_distance" format="dimension" />
        <!--扩散最大半径-->
        <attr name="spread_max_radius" format="dimension" />
        <!--扩散最小半径-->
        <attr name="spread_min_radius" format="dimension" />
        <!--扩散延迟间隔-->
        <attr name="spread_delay_milliseconds" format="float" />
        <!--最大波纹数-->
        <attr name="spread_circle_count" format="integer" />
        <!--可以设置背景	-->
        <attr name="spread_src" format="reference" />
        <!--背景宽度-->
        <attr name="spread_bg_width" format="dimension" />
        <!--背景高度-->
        <attr name="spread_bg_height" format="dimension" />
        <!--扩散粒度-->
        <attr name="spread_gain" format="float" />
        <!--是否匀速模式-->
        <attr name="spread_uniform_mode" format="boolean" />
    </declare-styleable>
</resources>